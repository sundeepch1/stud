package sorting;

public class BubleSort {

	public static void main(String[] args) {
		
		int arr[] = {5, 1, 6, 2, 4, 3};
		
		bubbleSort(arr);
	}

	private static void bubbleSort(int[] arr) {
		
		for (int i = 0; i < arr.length; i++) {
			for (int j = 0; j < arr.length -i -1; j++) {
				
			}
		}
	}

}
