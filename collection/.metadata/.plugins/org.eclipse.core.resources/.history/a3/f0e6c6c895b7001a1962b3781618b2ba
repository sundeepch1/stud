package com.hashmap;

import java.util.HashMap;
import java.util.Map;

public class HashMapClear {

	public static void main(String[] args) {
		Map<String,Integer> map = new HashMap<>();
		Map<Employee,Integer> emp = new HashMap<>();
		
		String s1 = "sundeep";
		String s2="sundeep";
		
		Employee e1=new Employee(3);
		Employee e2= new Employee(4);
		
		System.out.println(s1.hashCode() + "     " + s2.hashCode());
		
		map.put(s1,1);
		map.put(s2,2);
		
		emp.put(e1, 4);
		emp.put(e2, 4);
		
		System.out.println("Map = " + map.size());
		System.out.println("EMP = " + map.size());
				

	}

}

class Employee{
	private int id;
	public Employee(int id) {
		this.id = id;
	}
}
