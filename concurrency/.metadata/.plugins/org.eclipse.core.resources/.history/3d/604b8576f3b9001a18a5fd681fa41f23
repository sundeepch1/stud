package concurrency.thread.local;

public class LocalThreadComparison {
	public static void main(String ...args) {
		
	}
}

class SimpleRunnable implements Runnable{
	private Counter counter = new Counter();
	
	@Override
	public void run() {
		for (int i = 0; i < 5; i++) {
			counter.increment();
		}
	}
}

class Counter{
	private int count = 0;
	
	public void increment() {
		count++;
	}
	
	public int getCount() {
		return count;
	}
}
